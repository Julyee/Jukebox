{
  "name": "julyee.jukebox",
  "version": "1.0.0",
  "description": "Apple Music Player and Remote",
  "main": "src/index.js",
  "author": "Julyee Agura",
  "license": "MIT",
  "scripts": {
    "build": "npm-run-all --parallel build:**",
    "build:dev": "webpack",
    "build:dev:browser": "cross-env TARGET='browser' webpack",
    "build:prod": "cross-env NODE_ENV='production' webpack -p",
    "build:prod:browser": "cross-env NODE_ENV='production' TARGET='browser' webpack -p",
    "start": "cross-env TARGET='browser' webpack-dev-server --host 0.0.0.0 --port 8081 --content-base ./www --hot --disableHostCheck"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-latest": "^6.24.1",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "eslint": "^5.1.0",
    "eslint-loader": "^2.1.0",
    "file-loader": "^2.0.0",
    "jsonwebtoken": "^8.3.0",
    "node-sass": "^4.9.2",
    "npm-run-all": "^4.1.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.21.0",
    "webpack": "^4.16.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "@fortawesome/free-brands-svg-icons": "^5.3.1",
    "@fortawesome/free-solid-svg-icons": "^5.3.1",
    "mithril": "^1.1.6",
    "polythene-css": "^1.3.2",
    "polythene-mithril": "^1.3.2"
  }
}
